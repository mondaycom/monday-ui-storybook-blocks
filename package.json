{
  "name": "vibe-storybook-components",
  "version": "0.0.2",
  "description": "Collection of Vibe's Storybook components",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:mondaycom/vibe-storybook-components.git"
  },
  "sideEffects": [
    "*.scss",
    "*.css"
  ],
  "main": "dist/index.js",
  "files": [
    "dist/",
    "src/",
    "README.md"
  ],
  "scripts": {
    "build": "rollup -c --bundleConfigAsCjs",
    "release": "storybook build",
    "lint": "eslint \"./src/**/*.{js,jsx,ts,tsx}\"",
    "lint-fix": "eslint \"./src/**/*.{js,jsx,ts,tsx}\" --fix",
    "plop": "plop",
    "start": "yarn build --watch",
    "storybook": "storybook dev -p 6005",
    "test": "jest --passWithNoTests",
    "test:update": "yarn test -- -u"
  },
  "browserslist": [
    "extends browserslist-config-monday"
  ],
  "resolutions": {
    "react": "17.0.2"
  },
  "dependencies": {
    "@storybook/addon-actions": "^7.1.1",
