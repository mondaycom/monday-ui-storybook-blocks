    "react-dom": ">=16.9.0"
  },
  "changelog": {
    "labels": {
      "PR: Bugfix 🐛": "Bug Fixes",
      "PR: New Feature 🕹": "New Features",
      "PR: Dependencies 🛠": "Dependency Upgrades",
      "PR: Documentation 📖": "Documentation",
      "PR: Internal 🏠": "Internal Changes",
      "PR: Breaking Changes 💥": "Breaking Changes"
    }
  }
